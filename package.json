{
  "name": "langchain-toon-output-parser",
  "version": "0.1.0",
  "description": "LangChain community add-on for structured output parsing using TOON format with Zod schema validation",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "type": "module",
  "scripts": {
    "build": "tsc",
    "test": "vitest run",
    "test:watch": "vitest",
    "typecheck": "tsc --noEmit",
    "example:basic": "tsx examples/basic-usage.ts",
    "example:nested": "tsx examples/nested-objects.ts",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "langchain",
    "toon",
    "structured-output",
    "llm",
    "zod",
    "output-parser",
    "token-efficiency"
  ],
  "author": "Malte Pralle",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/mpralle/langchain-toon-output-parser"
  },
  "dependencies": {
    "@langchain/core": ">=0.2.0",
    "@toon-format/toon": "^2.0.1",
    "zod": "^3.22.0"
  },
  "devDependencies": {
    "@langchain/mistralai": "^0.1.0",
    "@types/node": "^20.10.0",
    "dotenv": "^17.2.3",
    "tsx": "^4.7.0",
    "typescript": "^5.3.0",
    "vitest": "^1.0.0"
  },
  "peerDependencies": {
    "@langchain/core": ">=0.2.0"
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ]
}